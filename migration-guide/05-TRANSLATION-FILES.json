      "pages/about.json": {
        "en": {
          "title": "About the Interactive Global Goods Guide",
          "subtitle": "A digital health marketplace where supply and demand meet to accelerate health equity through open source global goods.",
          "digitalSquare": {
            "title": "About Digital Square",
            "content": "Digital Square is a digital health marketplace–or 'place'–where supply and demand meet to accelerate health equity. We align investors and government agencies around a shared vision for digital health, promote the development and adoption of digital public goods for health, and strengthen national digital health governance.",
            "role": "Digital Square plays a unique role as a neutral convener of donors, government leaders, technical innovators, and entrepreneurs. Since 2016, we have collaborated with hundreds of stakeholders across 33 countries, supporting 36 mature global goods through our Open Application Process.",
            "learnMore": "Visit digitalsquare.org"
          },
          "globalGoods": {
            "title": "What Are Global Goods?",
            "definition": "Adaptable digital health tools that can be used across countries and contexts to help address key health system challenges. These free and open source tools can be used across different health programs, reducing fragmentation and accelerating health impact.",
            "types": {
              "software": {
                "title": "Software",
                "description": "Free and open source tools used for managing, analyzing, or transmitting health-related data, with proven utility in multiple environments."
              },
              "services": {
                "title": "Services",
                "description": "Software-as-a-service tools that manage, transmit, or analyze health data, freely accessible and adhering to open data principles."
              },
              "content": {
                "title": "Content",
                "description": "Resources, toolkits, or data standards available under open licenses for improving health data management capabilities."
              }
            }
          },
          "benefits": {
            "title": "Benefits and Impact",
            "content": "Global goods save lives by enabling collaboration between organizations, reducing vendor lock-in, sharing development costs, and promoting interoperability. Many support OpenHIE architecture standards for improved data analysis and continuity of care.",
            "examples": "For example, a global good used for HIV case management can also be used to manage malaria or tuberculosis care."
          },
          "process": {
            "title": "How Tools Become Global Goods",
            "content": "Digital health tools can apply anytime via our application portal, with quarterly reviews by our Peer Review Committee and Global Goods Review Board.",
            "requirements": "Tools must be published under open licenses, supported by strong communities, funded by multiple sources, deployed at scale, used in multiple countries, and designed for interoperability.",
            "apply": "Submit an Application",
            "contact": "Contact Digital Square Team"
          }
        }
